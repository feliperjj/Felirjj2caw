<body>
<div id='Lugar'> </div>
<script type="text/javascript">
{
var num1,num2,soma;
num1=parseInt(prompt("Digite o primeiro Numero"));
num2=parseInt(prompt("Digite o segundo Numero"));
soma=num1+num2;
document.getElementById('Lugar').innerHTML = soma;
}
</script>

